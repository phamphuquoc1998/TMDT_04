@model IEnumerable<TMDT.Models.Category>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";

}
<button class="btn btn-success btn-sm">
    @Html.ActionLink("Create New", "Create")
</button>
<table class="table table-striped">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CateName)
        </th>
        <th>công cụ</th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CateName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.CateID }) |
            @Html.ActionLink("Details", "Details", new { id=item.CateID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.CateID })
        </td>
    </tr>
}

</table>
